 function marcaincorrecta(){
    document.getElementById('palabra').style="display: block;"
    document.getElementById('bien').style="display: block;"
}

function Cambio(){
    document.getElementById('palabra').style="display: none;"
    document.getElementById('palabra2').style="display: block;"
}

function Cambio2(){
     document.getElementById('palabra3').style="display: none;"
     document.getElementById('palabra4').style="display: block;"
}


